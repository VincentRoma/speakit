{% trans_default_domain 'FOSUserBundle' %}
<div id="register">
<div class="container">
  <img class="img-responsive center-block" src="{{ asset('image/register.jpg') }}" alt="Paris">
    <div class="row">
        <div class="col-md-6 col-md-offset-3 text-center jumbotron" id="myjumbotron">
          <h2>Lancez-vous !</h2>
          <form action="{{ path('fos_user_registration_register') }}" {{ form_enctype(form) }} method="POST" class="form-signin">
          {{ form_widget(form) }}
          <div>
            <input  class="btn-default btn btn-edu btn-block btn-lg btn-lg" type="submit" value="{{ 'registration.submit'|trans }}" />
          </div>
      </form>
      </div>
    </div>
  </div>
</div>

<script>
    if(window.location.hostname !== 'localhost'){
        mixpanel.track("Register Start");
    }else{
        console.log("fake mixpanel localhost");
    }
</script>
